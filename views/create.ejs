    <title>Cadastro</title>
  </head>
  <body>
    <h1>Cadastro de consulta</h1>
    <hr />
    <form method="POST" action="http://localhost:8686/create">
      <input
        type="text"
        name="name"
        id="name"
        placeholder="Nome do paciente"
        class="form-control"
      />
      <br />
      <input
        type="email"
        name="email"
        id="email"
        placeholder="xxxxx@email.com"
        class="form-control"
      />
      <br />
      <input
        type="text"
        name="cpf"
        id="cpf"
        placeholder="CPF"
        class="form-control cpf"
      />
      <br />
      <input
        type="text"
        name="description"
        id="description"
        class="form-control"
        placeholder="Descrição..."
      />
      <br />
      <input
        type="date"
        name="date"
        id="date"
        class="form-control"
        placeholder="12/02/2022"
      />
      <br />
      <input
        type="time"
        name="time"
        id="time"
        class="form-control"
        placeholder="xx:xx"
      />
      <br />
      <button type="submit" class="btn btn-success col-12">Agendar</button>
    </form>
  </body>
  <script
    src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"
    type="text/javascript"
  ></script>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
